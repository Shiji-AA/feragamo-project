      <!DOCTYPE html>
      <html lang="en">
      <head>
          <!-- Add your head content here -->
          <title>Your Page Title</title>
          <!-- Include necessary CSS, Bootstrap, or other libraries -->
          <link rel="stylesheet" href="path/to/bootstrap.min.css">
          <!-- Add any other stylesheets or scripts -->
      </head>
      <body>
          <section class="mt-50 mb-50">
              <div class="container">
                  <div class="row flex-row-reverse">
                      <div class="col-lg-9-l">
                          <% for (let i = 0; i < product.length; i++) { %>
                              <!-- Check if it's an even index to start a new row -->
                              <% if (i % 2 === 0) { %>
                                  <div class="row">
                              <% } %>
      
                              <!-- PRODUCTS LIST -->
                              <div class="col-md-6">
                                  <div class="product-list mb-50">
                                      <div class="product-cart-wrap">
                                          <!-- photo -->
                                          <div class="product-img-action-wrap">
                                              <div class="product-img product-img-zoom">
                                                  <div class="product-img-inner">
                                                      <a href="/product-detail?id=<%=product[i]._id %>">
                                                          <img src="/productImages/<%= product[i].images[0] %>"
                                                              width="400px" height="400px" class="img-sm img-thumbnail"
                                                              alt="Item">
                                                      </a>
                                                  </div>
                                              </div>
                                              <div class="product-badges product-badges-position product-badges-mrg">
                                                  <span class="hot">Hot</span>
                                              </div>
                                          </div>
                                          <!-- photo ends -->
      
                                          <div class="product-content-wrap">
                                              <div class="product-category"></div>
                                              <br>
                                              <h2>
                                                  <a href="/product-detail?id=<%=product[i]._id %>">
                                                      <%= product[i].name %>
                                                  </a>
                                              </h2>
                                              <!-- price -->
                                              <div class="product-price">
                                                  <span>₹ <%= product[i].price %></span>
                                                  <span class="old-price">
                                                      ₹ <%= ((product[i].price)+((product[i].price)*.1)) %>
                                                  </span>
                                              </div>
                                              <div class="product-gender">
                                                  <%= product[i].gender %>
                                              </div>
      
                                              <div class="product-category">
                                                  <%= product[i].category %>
                                              </div>
      
                                              <!-- description -->
                                              <div class="product-description">
                                                  <%= product[i].description %>
                                              </div>
                                              <form action="/addToCart?productId=<%=product[i]._id%>" method="POST">
                                                  <!-- add to cart -->
                                                  <!-- ... Add your add to cart button or functionality here -->
                                              </form>
                                          </div>
                                      </div>
                                  </div>
                              </div>
      
                              <!-- Check if it's the last or an odd index to close the row -->
                              <% if ((i + 1) % 2 === 0 || i === product.length - 1) { %>
                                  </div>
                              <% } %>
                          <% } %>
                      </div>
      
                   
                  </div>
              </div>
          </section>
      
          <!-- Add your footer, additional scripts, or closing body and HTML tags -->
      </body>
      
